{
  "localfile": "BC-childcare.csv",
  "schema_groups": ["childcare", "address", "geocoordinates"],
  "source": "https://catalogue.data.gov.bc.ca/dataset/child-care-map-data#",
  "licence": "https://www2.gov.bc.ca/gov/content/data/open-data/open-government-licence-bc",
  "provider": "Province of British Columbia",
  "encoding": "utf-8",
  "format": {
      "type": "csv",
      "delimiter": ",",
      "quote": "\""
  },
  "schema": {
      "childcare" : {
          "name": "NAME",
          "source_id": "FAC_PARTY_ID",
          "licence_status": "SRVC_LICPRE_YN",
          "infant": "SRVC_UNDER36_YN",
          "toddler": "toddler",
          "school_age": "SRVC_OOS_GR1_AGE12_YN"
      },
      "address" : {
          "city": "CITY",
          "full_address": [
              "ADDRESS_1",
              "ADDRESS_2"
          ],
          "postal_code": "POSTAL_CODE"
      },

      "geocoordinates" : {
          "longitude": "LATITUDE",
          "latitude": "LONGITUDE"
      }
  }
}
